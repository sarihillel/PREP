
<script id="template" type="text/x-jquery-tmpl">
    <div class="_repeating_block" data-record-key="${model.ReleaseChecklistAnswerID}" data-QID="${model.ReleaseChecklistAnswerID}">
        <ul class="_repeating_rows" id="${model.ReleaseChecklistAnswerID}" style="width:97%">
            <li class="width-9"><div class="col-lg-12 col-md-12 col-xs-12 question-code">${model.QuestionCode}</div></li>
            <li class="_width_Qtxt">
                <div class="_padding-0" style="display:inline-block">
                    <p>${model.QuestionText}</p>
                    <div class="question_icons">
                        <a href="javascript:;" data-placement="bottom" data-toggle="popover" data-tri-gger="focus" data-content="${model.QuestionInfo}">
                            <img class="info_little" src="/Content/CheckList/icons/info.png" width="14" height="14">
                        </a>&nbsp;

                    </div>
                </div>
            </li>
            <li class="_width_risk_level">
                <input type="hidden" id="actualComplation" value="${model.ActualComplation}" />

                <div class="btn-group">
                    <button type="button" class="actualComplationBtn btn btn-default _border-radius-0 {{if 0===model.ActualComplation}} selected-btn {{/if}}" data-val="0">0</button>
                    <button type="button" class="actualComplationBtn btn btn-default {{if 10===model.ActualComplation}} selected-btn {{/if}}" data-val="10">10</button>
                    <button type="button" class="actualComplationBtn btn btn-default {{if 20===model.ActualComplation}} selected-btn {{/if}}" data-val="20">20</button>
                    <button type="button" class="actualComplationBtn btn btn-default {{if 30===model.ActualComplation}} selected-btn {{/if}}" data-val="30">30</button>
                    <button type="button" class="actualComplationBtn btn btn-default {{if 40===model.ActualComplation}} selected-btn {{/if}}" data-val="40">40</button>
                    <button type="button" class="actualComplationBtn btn btn-default {{if 50===model.ActualComplation}} selected-btn {{/if}}" data-val="50">50</button>
                    <button type="button" class="actualComplationBtn btn btn-default {{if 60===model.ActualComplation}} selected-btn {{/if}}" data-val="60">60</button>
                    <button type="button" class="actualComplationBtn btn btn-default {{if 70===model.ActualComplation}} selected-btn {{/if}}" data-val="70">70</button>
                    <button type="button" class="actualComplationBtn btn btn-default {{if 80===model.ActualComplation}} selected-btn {{/if}}" data-val="80">80</button>
                    <button type="button" class="actualComplationBtn btn btn-default {{if 90===model.ActualComplation}} selected-btn {{/if}}" data-val="90">90</button>
                    <button type="button" class="actualComplationBtn btn btn-default _border-radius-0 {{if 100===model.ActualComplation}} selected-btn {{/if}}" data-val="100">100</button>
                </div>

                <select class="form-control _top_8 _red risk-level-ddl" id="sel1" data-selected="${model.RiskLevelID}">
                    {{each(prop,val) riskLevelList}}
                    <option value="${prop}">${val}</option>
                    {{/each}}
                </select>
            </li>
            <li class="width-35  _comment">
                <textarea class="form-control width-100 pull-left comment-input comment" rows="2" disabled >${model.Comments}</textarea>
                <div class="_width_93 pull-left">
                    @*<a href="javascript:;" class="_font-10 pull-left _view_log view-log" data-placement="bottom" data-trigger="focus" data-toggle="popover"
                        data-releaseChecklistAnswer="${model.ReleaseChecklistAnswerID}" data-content="">View Log >></a>*@ <a href="javascript:;" class="_font-10 pull-right _view_log add-comment">
                        Add Comment +
                    </a>

                    <a href="javascript:;" class="_font-10 pull-left _view_log view-log" data-placement="bottom" data-releaseChecklistAnswer="${model.ReleaseChecklistAnswerID}"
                       data-trigger="focus" data-content="">View Log >></a>

            </li>
        </ul>

        <ul class="_font-10">
            <li class="width-9">&nbsp;</li>
            <li class="_width_controls">
                <div class="_border_bottom">
                    <div class="pull-left" style="display:inline">
                        <span>
                            <a href="javascript:;" class="hideShowRow pull-left  _padding-10 _more">Show More Details +</a>&nbsp;&nbsp;
                        </span>
                    </div>
                    <span class="hide-row" style="display:none;">
                        <div class="width-14">
                            <label for="area" class="pull-left _padding-10">Area:&nbsp;</label>
                            <input type="text" disabled class="form-control disabled-input more-details-width" id="sel1" value="${model.AreaName}" title="${model.AreaName}" />
                            @*{{each areaList}}
                                    <option value="${AreaID}" {{if AreaID===model.AreaID}} selected="selected" {{/if}}>${Name}</option>
                                    {{/each}}

                                </input>*@
                        </div>
                        <div class="width-14">
                            <label for="area" class="pull-left  _padding-10">Sub Area:&nbsp;</label>
                            <input type="text" disabled class="form-control disabled-input more-details-width" id="sel1" value="${model.SubAreaName}" title="${model.SubAreaName}" />
                            @*<select class="form-control _repeating_row_dropdowns ddlSubArea" id="sel1" data-isUpdated="false">
                                    <option value="${model.SubAreaID}" selected="selected">${model.SubAreaName}</option>
                                </select>*@

                        </div>
                        <div class="width-14">
                            <label for="area" class="pull-left _padding-10">Responsibility:&nbsp;</label>
                            <select class="form-control pull-left input-sm _repeating_row_dropdowns ddlResponsibility more-details-width" id="sel1" data-selected="${model.Responsibility}">
                                {{each(prop,val) responsibility}}
                                <option value="${prop}">${val}</option>
                                {{/each}}
                            </select>
                        </div>
                        <div class="width-14">
                            <label for="area" class="pull-left _padding-10">Question Owner:&nbsp;</label>
                            <input type="hidden" value="${model.QuestionOwnerID}" id="QuestionOwnerID" data-hidden="${model.ReleaseChecklistAnswerID}" />
                            <input type="text" class="form-control input-sm pull-left  _user userDialog disabled-input more-details-width" disabled id="inputsm" value="${model.QuestionOwnerName}"
                                   data-displayed="${model.ReleaseChecklistAnswerID}" title="${model.QuestionOwnerName}">
                        </div>



                        <div class="width-14 _date-calender" title="By default Handling Start Date = Previous Milestone Date' + &#013 (Milestone Date- Previous Milestone Date) * Ratio between Milestones.">
                            <label for="area" class="pull-left _padding-10">&nbsp;Handling Start Date:&nbsp;</label>
                            <input type='text' class="form-control input-sm pull-left _calender HandlingStartDatePopover more-details-width"  value="${formatDate(model.HandlingStartDate)}" data-date="${model.HandlingStartDate}" disabled="disabled">

                            @*<div id="popoverExampleTwoHiddenContent" style="display: none">
                                <div class="form-group _padding-10 pull-left">
                                    <label class="_font-10 _label">Change Effective Date:</label>
                                    <input type='text' class="form-control input-sm pull-left _date HandlingStartDatePopoverInput" onmouseover="bindDateTimePicker($(this))" />
                                </div>
                                <div class="form-group _padding-10 pull-left">
                                    <button type="button" class="btn btn-info _no-border-radius-btn _popover_btns" onclick="HandlingSrartDateClose()">Cancel</button>
                                    <button type="button" class="btn btn-info _no-border-radius-btn _popover_btns popoverSave" onclick="HandlingSrartDateSave()">Save</button>
                                    <button type="button" class="btn btn-info _no-border-radius-btn _popover_btn popoverReset" onclick="HandlingSrartDateReset(this)" data-QID="${model.QuestionID}">Resume Formula</button>
                                </div>
                            </div>*@
                        </div>
                    </span>
                </div>
            </li>
        </ul>
    </div>
</script>
