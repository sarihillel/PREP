            @Html.ValidationMessageFor(m => m.FullNameEmployee2, "", new { @class = "text-danger" })
            @Html.EditorFor(m => m.EmployeeID2, new { htmlAttributes = new { @class = "edit-contol hidden-employeeId edit-people " + @Html.NameFor(m=>m.Employee2)+"_hdnId", @hidden = "hidden" } })
            @Html.ValidationMessageFor(m => m.EmployeeID2, "", new { @class = "text-danger", @hidden = "hidden" })
            <div class="userIcon sprite icon-39" id="@Html.NameFor(m=>m.Employee2)" ></div>
            <div class="floatLeft sprite icon-66 minusIcon" ></div>
        </div>
        <input type="button" class="_addUser btn-link aStyle"   id="@Html.NameFor(m=>m.IsAddUser)" value="Add user +" />
    </div>
</div>
<hr class="mewHr" />

